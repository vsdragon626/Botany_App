        <activity
            android:name="com.cs.planttrack.ManualLocationActivity"
            android:label="@string/title_activity_manual_location" >
        </activity>
        <activity
            android:name="com.cs.planttrack.Entrymap_1_3"
            android:label="@string/title_activity_entrymap_1_3" >
